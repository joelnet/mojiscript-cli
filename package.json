{
  "name": "mojiscript-cli",
  "version": "0.0.1",
  "description": "MojiScript CLI",
  "author": "Joel Thoms",
  "license": "MIT",
  "bin": {
    "moji": "./src/index.mjs"
  },
  "scripts": {
    "start": "node --experimental-modules --no-warnings src/index.mjs",
    "build": "eslint . --ext .js,.mjs",
    "test": "jest",
    "watch": "esw -w --fix --ext .js,.mjs"
  },
  "dependencies": {
    "eslint-watch": "^4.0.2",
    "mojiscript": "^0.10.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.1",
    "eslint-config-mojiscript": "^1.2.0",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-prefer-arrow": "^1.1.3",
    "jest": "^23.5.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    }
  }
}
